{
  "name": "student",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e ../../.env.local -- next dev --port 3001",
    "dev:prod": "dotenv -e ../../.env.production -- next dev --port 3001",
    "build": "dotenv -e ../../.env.production -- next build",
    "postbuild": "dotenv -e ../../.env.production -- next-sitemap",
    "pages:build": "dotenv -e ../../.env.production -- npx @cloudflare/next-on-pages@1",
    "preview": "npx wrangler pages dev .vercel/output/static --port 3001",
    "deploy": "npx wrangler pages deploy .vercel/output/static",
    "start": "next start --port 3001",
    "lint": "../../node_modules/.bin/biome check .",
    "format": "../../node_modules/.bin/biome format --write .",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@lukemorales/query-key-factory": "^1.3.4",
    "@tanstack/react-query": "^5.62.14",
    "config-tailwind": "workspace:*",
    "next": "15.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "shared": "workspace:*",
    "sonner": "^2.0.7",
    "ui": "workspace:*",
    "utils": "workspace:*"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "next-sitemap": "^4.2.3",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vercel": "^50.15.1"
  }
}
