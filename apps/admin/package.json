{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e ../../.env.local -- next dev --port 3002",
    "dev:prod": "dotenv -e ../../.env.production -- next dev --port 3002",
    "build": "dotenv -e ../../.env.production -- next build",
    "postbuild": "next-sitemap",
    "pages:build": "dotenv -e ../../.env.production -- npx @cloudflare/next-on-pages@1",
    "preview": "npx wrangler pages dev .vercel/output/static --port 3002",
    "deploy": "npx wrangler pages deploy .vercel/output/static",
    "start": "next start --port 3002",
    "lint": "../../node_modules/.bin/biome check .",
    "format": "../../node_modules/.bin/biome format --write ."
  },
  "dependencies": {
    "config-tailwind": "workspace:*",
    "next": "15.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "ui": "workspace:*"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "next-sitemap": "^4.2.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
